{
    "compilerOptions": {
      "target": "ES5",
      "module": "ESNext",
      "allowJs": true,
      "jsx": "react",
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noImplicitAny": false,
      "outDir": "./dist",
      "moduleResolution": "node"
    }
}
  